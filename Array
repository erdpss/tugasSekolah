
package array;


public class LatihanArray {
    public static void main(String[] args) {
        
        int nilai [];
        nilai = new int [3];
        nilai [0] = 2;
        nilai [1] = 3;
        nilai [2] = 5;
        double ratarata = 0.0;
        
        for (int i = 0; i < nilai.length; i++) {
            ratarata += nilai[i];
        }
        ratarata /= nilai.length;
        System.out.println("Nilai rata - rata : " + ratarata );
}
}
